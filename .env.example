# Giscus Comments Configuration
# Get these values from https://giscus.app after setting up Giscus for your repository

# Required: Repository ID (get from giscus.app)
NEXT_PUBLIC_GISCUS_REPO_ID=

# Required: Discussion Category ID (get from giscus.app)
NEXT_PUBLIC_GISCUS_CATEGORY_ID=

# Optional: Discussion Category Name (default: "General")
NEXT_PUBLIC_GISCUS_CATEGORY=General

# Notion Integration (if using)
NOTION_API_KEY=
NOTION_DATABASE_ID=
